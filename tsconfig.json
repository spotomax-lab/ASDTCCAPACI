{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": false,
    "paths": {
      "*": ["*", "*.js", "*.ts", "*.tsx"],
      "@/*": ["./*"],
      "@config/*": ["./config/*"],
      "@admin/*": ["./admin/*"],
      "@screens/*": ["./screens/*"]
    }
  }
}